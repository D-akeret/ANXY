<Project>
  <Project.ExportOptions Namespace="ANXY" Class="BasicUI" OutputPath="C:\Users\Gamer\Desktop" />
  <VerticalStackPanel Spacing="20" HorizontalAlignment="Center" VerticalAlignment="Center" MinWidth="500" Padding="50" Background="#0000FF44">    
