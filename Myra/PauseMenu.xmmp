    <TextButton Text="Resume Game" Padding="10" HorizontalAlignment="Stretch" />
    <TextButton Text="Reset / New Game" Padding="10" HorizontalAlignment="Stretch" />
    <TextButton Text="Controls" Padding="10" HorizontalAlignment="Stretch" />
    <TextButton Text="Credits" Padding="10" HorizontalAlignment="Stretch" />
    <TextButton Text="Exit Game" Padding="10" HorizontalAlignment="Stretch" />
  </VerticalStackPanel>
</Project>